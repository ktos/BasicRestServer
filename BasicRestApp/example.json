{
	Host: "localhost",
	Port: 8000,
	MaxThreads: 5,
	Routes:
		[
			{
				Method: "GET",
				Path: "/test/(?<regex>.*)$",
				File: "regex",
				"__comment": "may be 'regex' when a file name is given in group named 'regex'"
			},
			{
				Method: "GET",
				Path: "/test2$",
				Command: "echo hello"
			},
			{
				Method: "GET",
				Path: "/test3$",
				File: "test.txt",
				Response:
				{
					Type: "text/html"
				}
			}
		]
}